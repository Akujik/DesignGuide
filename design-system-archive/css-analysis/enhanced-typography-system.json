{
  "fontFamilies": {
    "primary": {
      "name": "Inter System",
      "stack": [
        "Inter",
        "-apple-system",
        "BlinkMacSystemFont",
        "Segoe UI",
        "Roboto",
        "Helvetica Neue",
        "Arial",
        "sans-serif"
      ],
      "fallback": "system-ui, sans-serif",
      "characteristics": {
        "xHeight": "medium",
        "weightRange": "100-900",
        "languageSupport": [
          "latin",
          "cyrillic",
          "greek",
          "vietnamese"
        ],
        "rendering": "optimized for screens"
      }
    },
    "secondary": {
      "name": "Karma System",
      "stack": [
        "Karma",
        "Georgia",
        "Times New Roman",
        "Times",
        "serif"
      ],
      "fallback": "Georgia, serif",
      "characteristics": {
        "xHeight": "medium-high",
        "weightRange": "300-700",
        "languageSupport": [
          "latin"
        ],
        "rendering": "classic, readable"
      }
    },
    "mono": {
      "name": "Inter Tight System",
      "stack": [
        "Inter Tight",
        "SF Mono",
        "Monaco",
        "Inconsolata",
        "Roboto Mono",
        "Source Code Pro",
        "Fira Code",
        "Courier New",
        "monospace"
      ],
      "fallback": "Consolas, Monaco, monospace",
      "characteristics": {
        "xHeight": "high",
        "weightRange": "300-700",
        "languageSupport": [
          "latin",
          "programming"
        ],
        "rendering": "code-optimized"
      }
    },
    "display": {
      "name": "Figtree System",
      "stack": [
        "Figtree",
        "Inter",
        "Helvetica Neue",
        "Arial",
        "sans-serif"
      ],
      "fallback": "Inter, sans-serif",
      "characteristics": {
        "xHeight": "medium",
        "weightRange": "400-900",
        "languageSupport": [
          "latin"
        ],
        "rendering": "heading-optimized"
      }
    },
    "chinese": {
      "simplified": {
        "stack": [
          "PingFang SC",
          "Hiragino Sans GB",
          "Microsoft YaHei",
          "Source Han Sans SC",
          "Noto Sans CJK SC",
          "WenQuanYi Micro Hei",
          "sans-serif"
        ]
      },
      "traditional": {
        "stack": [
          "PingFang TC",
          "Hiragino Sans CNS",
          "Microsoft JhengHei",
          "Source Han Sans TC",
          "Noto Sans CJK TC",
          "sans-serif"
        ]
      }
    }
  },
  "fontSizes": {
    "base": {
      "xs": {
        "value": "12px",
        "rem": "0.75rem",
        "lineHeight": "1.5",
        "use": [
          "caption",
          "metadata",
          "small print"
        ]
      },
      "sm": {
        "value": "14px",
        "rem": "0.875rem",
        "lineHeight": "1.5",
        "use": [
          "small text",
          "labels",
          "secondary info"
        ]
      },
      "base": {
        "value": "16px",
        "rem": "1rem",
        "lineHeight": "1.5",
        "use": [
          "body text",
          "paragraphs",
          "default"
        ]
      },
      "lg": {
        "value": "18px",
        "rem": "1.125rem",
        "lineHeight": "1.5",
        "use": [
          "large body text",
          "subheadings"
        ]
      },
      "xl": {
        "value": "20px",
        "rem": "1.25rem",
        "lineHeight": "1.5",
        "use": [
          "large text",
          "small headings"
        ]
      },
      "2xl": {
        "value": "24px",
        "rem": "1.5rem",
        "lineHeight": "1.4",
        "use": [
          "headings",
          "large titles"
        ]
      },
      "3xl": {
        "value": "30px",
        "rem": "1.875rem",
        "lineHeight": "1.3",
        "use": [
          "main headings",
          "section titles"
        ]
      },
      "4xl": {
        "value": "36px",
        "rem": "2.25rem",
        "lineHeight": "1.2",
        "use": [
          "hero headings",
          "page titles"
        ]
      },
      "5xl": {
        "value": "48px",
        "rem": "3rem",
        "lineHeight": "1.1",
        "use": [
          "display headings",
          "hero text"
        ]
      },
      "6xl": {
        "value": "64px",
        "rem": "4rem",
        "lineHeight": "1.0",
        "use": [
          "mega headings",
          "brand text"
        ]
      }
    },
    "special": {
      "caption": {
        "value": "10px",
        "rem": "0.625rem",
        "lineHeight": "1.4",
        "use": [
          "image captions",
          "table headers"
        ]
      },
      "overline": {
        "value": "11px",
        "rem": "0.6875rem",
        "lineHeight": "1.4",
        "use": [
          "overline text",
          "badges"
        ]
      }
    }
  },
  "fontWeights": {
    "system": {
      "thin": {
        "value": 100,
        "name": "Thin"
      },
      "extraLight": {
        "value": 200,
        "name": "Extra Light"
      },
      "light": {
        "value": 300,
        "name": "Light"
      },
      "normal": {
        "value": 400,
        "name": "Normal"
      },
      "medium": {
        "value": 500,
        "name": "Medium"
      },
      "semiBold": {
        "value": 600,
        "name": "Semi Bold"
      },
      "bold": {
        "value": 700,
        "name": "Bold"
      },
      "extraBold": {
        "value": 800,
        "name": "Extra Bold"
      },
      "black": {
        "value": 900,
        "name": "Black"
      }
    },
    "usage": {
      "body": [
        400,
        500
      ],
      "headings": [
        600,
        700
      ],
      "display": [
        700,
        800,
        900
      ],
      "emphasis": [
        600,
        700
      ],
      "metadata": [
        300,
        400
      ],
      "navigation": [
        500,
        600
      ]
    },
    "semiDesign": {
      "regular": 400,
      "medium": 500,
      "semiBold": 600,
      "bold": 700
    }
  },
  "lineHeights": {
    "standard": {
      "none": {
        "value": 1,
        "name": "None"
      },
      "tight": {
        "value": 1.25,
        "name": "Tight"
      },
      "snug": {
        "value": 1.375,
        "name": "Snug"
      },
      "normal": {
        "value": 1.5,
        "name": "Normal"
      },
      "relaxed": {
        "value": 1.625,
        "name": "Relaxed"
      },
      "loose": {
        "value": 2,
        "name": "Loose"
      }
    },
    "pairings": {
      "12px": "1.5",
      "14px": "1.5",
      "16px": "1.5",
      "18px": "1.5",
      "20px": "1.5",
      "24px": "1.4",
      "30px": "1.3",
      "36px": "1.2",
      "48px": "1.1",
      "64px": "1.0"
    },
    "special": {
      "code": "1.4",
      "paragraph": "1.7",
      "dense": "1.25",
      "comfortable": "1.75"
    }
  },
  "letterSpacing": {
    "standard": {
      "tighter": {
        "value": "-0.05em",
        "name": "Tighter"
      },
      "tight": {
        "value": "-0.025em",
        "name": "Tight"
      },
      "normal": {
        "value": "0",
        "name": "Normal"
      },
      "wide": {
        "value": "0.025em",
        "name": "Wide"
      },
      "wider": {
        "value": "0.05em",
        "name": "Wider"
      },
      "widest": {
        "value": "0.1em",
        "name": "Widest"
      }
    },
    "usage": {
      "headings": [
        "-0.025em",
        "0"
      ],
      "uppercase": [
        "0.05em",
        "0.1em"
      ],
      "dense": [
        "-0.025em",
        "0"
      ],
      "comfortable": [
        "0",
        "0.025em"
      ],
      "display": [
        "-0.05em",
        "0"
      ]
    }
  },
  "responsive": {
    "fluid": {
      "minFont": "16px",
      "maxFont": "20px",
      "minViewport": "320px",
      "maxViewport": "1200px",
      "formula": "clamp(1rem, 2.5vw, 1.25rem)"
    },
    "breakpoints": {
      "mobile": {
        "h1": "28px",
        "h2": "24px",
        "h3": "20px",
        "body": "16px"
      },
      "tablet": {
        "h1": "32px",
        "h2": "28px",
        "h3": "24px",
        "body": "16px"
      },
      "desktop": {
        "h1": "42px",
        "h2": "36px",
        "h3": "30px",
        "body": "16px"
      }
    },
    "mediaQueries": {
      "mobile": "@media (max-width: 767px)",
      "tablet": "@media (min-width: 768px) and (max-width: 1023px)",
      "desktop": "@media (min-width: 1024px)"
    }
  },
  "accessibility": {
    "minimumFontSize": {
      "body": "16px",
      "interactive": "18px",
      "caption": "14px"
    },
    "contrast": {
      "AA-normal": "4.5:1",
      "AA-large": "3:1",
      "AAA-normal": "7:1",
      "AAA-large": "4.5:1"
    },
    "readability": {
      "lineHeight": "1.5-1.7",
      "paragraphSpacing": "1em",
      "wordSpacing": "0.1em",
      "maxWidth": "65-75ch"
    },
    "userPreferences": {
      "prefers-reduced-motion": "transitions: none",
      "prefers-contrast-high": "enhanced contrast",
      "font-size-adjust": "respect user settings"
    }
  },
  "multiLanguage": {
    "languageMapping": {
      "en": "primary",
      "zh-CN": "chinese.simplified",
      "zh-TW": "chinese.traditional",
      "zh-HK": "chinese.traditional",
      "ja": "japanese",
      "ko": "korean",
      "ar": "arabic",
      "he": "hebrew",
      "th": "thai",
      "vi": "primary"
    },
    "considerations": {
      "CJK": {
        "lineBreak": "strict",
        "letterSpacing": "0.05em",
        "fontWeight": "500"
      },
      "Arabic": {
        "direction": "rtl",
        "fontFamily": "Arial, sans-serif",
        "letterSpacing": "0"
      },
      "Thai": {
        "lineHeight": "1.6",
        "fontFamily": "Sarabun, sans-serif"
      }
    }
  }
}